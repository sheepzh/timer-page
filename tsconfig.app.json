{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    ".vitepress/**/*",
    "typings/**/*",
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "checkJs": false,
    "strictNullChecks": false,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}